            xmin = 354.5 
            xmax = 356.96 
            text = "<NOISE>" 
        intervals [107]:
            xmin = 356.96 
            xmax = 363.38 
            text = "Drawing back upon myself - this time unimpeded by the turbulent flux of magics -" 
        intervals [108]:
            xmin = 363.38 
            xmax = 364.03 
            text = "<NOISE>" 
        intervals [109]:
            xmin = 364.03 
            xmax = 372.35 
