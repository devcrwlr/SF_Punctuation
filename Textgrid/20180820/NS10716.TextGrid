            xmin = 431.81 
            xmax = 433.62 
            text = "<NOISE>" 
        intervals [133]:
            xmin = 433.62 
            xmax = 437.36 
            text = "The proof of this was his lack of action against me during the next ten seconds." 
        intervals [134]:
            xmin = 437.36 
            xmax = 439.05 
            text = "<NOISE>" 
        intervals [135]:
            xmin = 439.05 
            xmax = 441.17369780530464 
            text = "I was tense all the same until" 
