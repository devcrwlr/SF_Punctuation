            xmin = 449.43 
            xmax = 450.17 
            text = "<NOISE>" 
        intervals [139]:
            xmin = 450.17 
            xmax = 454.7033529673886 
            text = "Now, did I purposely exhale, purging my lungs of that pervasive death stench." 
        intervals [140]:
            xmin = 454.7033529673886 
            xmax = 455.27466278229423 
            text = "<NOISE>" 
        intervals [141]:
            xmin = 455.27466278229423 
            xmax = 461.17 
            text = "It seemed to cling to me even in this form, inspiring me to fly faster to escape it." 
        intervals [142]:
            xmin = 461.17 
