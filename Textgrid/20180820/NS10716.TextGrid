            text = "<NOISE>" 
        intervals [113]:
            xmin = 376.63 
            xmax = 384.54 
            text = "while I was in this small and somewhat more vulnerable form, but in my mind, I was confident that he was still unaware of my realization." 
        intervals [114]:
            xmin = 384.54 
            xmax = 385.22 
            text = "<NOISE>" 
        intervals [115]:
            xmin = 385.22 
            xmax = 390.69 
