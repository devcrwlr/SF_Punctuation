            text = "<NOISE>" 
        intervals [149]:
            xmin = 489.43 
            xmax = 498.40311005680513 
            text = "The winds at this elevation are often hazardous and unpredictable, but now they seemed to in accord of my will and propelled me forward," 
        intervals [150]:
            xmin = 498.40311005680513 
            xmax = 501.6623135295241 
