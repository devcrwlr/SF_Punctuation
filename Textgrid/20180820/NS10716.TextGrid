        intervals [151]:
            xmin = 501.6623135295241 
            xmax = 501.9739370649272 
            text = "<NOISE>" 
        intervals [152]:
            xmin = 501.9739370649272 
            xmax = 508.03 
            text = "small raft on wild rapids, but I was in control, keeping upright and holding direction." 
        intervals [153]:
            xmin = 508.03 
            xmax = 508.6 
            text = "<NOISE>" 
        intervals [154]:
            xmin = 508.6 
            xmax = 515.82 
            text = "In less than half the regular time, it usually took to travel the distance, I reached the castle in swelled human form," 
        intervals [155]:
            xmin = 515.82 
